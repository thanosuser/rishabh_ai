

_get_repolink () {
    local regex
    regex='(https?)://github.com/.+/.+'
    if [[ $thanosuser_REPO == "rishabh_ai" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL3RoYW5vc3VzZXIvcmlzaGFiaF9haS9hcmNoaXZlL21haW4uemlw" | base64 -d
    elif [[ $thanosuser_REPO == "rishabh_ai" ]]
    then
        echo "aHR0cHM6Ly9naXRodWIuY29tL3RoYW5vc3VzZXIvcmlzaGFiaF9haS9hcmNoaXZlL21haW4uemlw" | base64 -d
    elif [[ $thanosuser_REPO =~ $regex ]]
    then
        if [[ $thanosuser_REPO_BRANCH ]]
        then
            echo "${thanosuser_REPO}/archive/${thanosuser_REPO_BRANCH}.zip"
        else
            echo "${thanosuser_REPO}/archive/main.zip"
        fi
    else
        echo "aHR0cHM6Ly9naXRodWIuY29tL3RoYW5vc3VzZXIvcmlzaGFiaF9haS9hcmNoaXZlL21haW4uemlw" | base64 -d
    fi
}


_pro_bot () {
    local zippath
    zippath="main.zip"
    echo "  ğŸ’• Downloading ï¼´ï¼¨Î›ï¼®â™¢ï¼³ï¼ï¼°ï¼²â™¢ Source Code... ğŸ’•"
    wget -q $(_get_repolink) -O "$zippath"
    echo "  ğŸ’“Unpacking Data...ğŸ’“"
    mainPATH=$(zipinfo -1 "$zippath" | grep -v "/.");
    unzip -qq "$zippath"
    echo " ğŸ’” Download Complete Now Installing ğŸ’”"
    echo "  âš¡ INSTALLING ï¼´ï¼¨Î›ï¼®â™¢ï¼³ï¼ï¼°ï¼²â™¢ âš¡"
    rm -rf "$zippath"
    sleep 5
    cd $mainPATH
    echo "    âš¡Starting ï¼´ï¼¨Î›ï¼®â™¢ï¼³ï¼ï¼°ï¼²â™¢âš¡     "
    echo " ğŸ’ NOW WAIT 3 MIN INSTALLING ALL THANOS PLUGIN ğŸ’"

    python3 ../setup/updater.py ../requirements.txt requirements.txt
    python3 -m userbot
}

_pro_bot
